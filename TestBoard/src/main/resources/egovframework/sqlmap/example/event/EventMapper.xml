<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="egovframework.example.event.service.impl.EventMapper">


    <select id="selectProductById" parameterType="egovframework.example.event.service.EventVO" resultType="egovframework.example.event.service.EventVO">
        SELECT ID, NAME, PRICE, IMAGE_PATH AS IMAGEPATH, SUB_ID, DESCRIPTION FROM PRODUCTIMG WHERE id = #{id}
    </select>

</mapper>